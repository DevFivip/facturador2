{% set invoice = doc.invoice -%}
<?xml version="1.0" encoding="utf-8" standalone="no"?>
<Invoice xmlns="urn:oasis:names:specification:ubl:schema:xsd:Invoice-2"
         xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
         xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
         xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
         xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2">
    <ext:UBLExtensions>
        <ext:UBLExtension>
            <ext:ExtensionContent/>
        </ext:UBLExtension>
    </ext:UBLExtensions>
    <cbc:UBLVersionID>2.1</cbc:UBLVersionID>
    <cbc:CustomizationID>2.0</cbc:CustomizationID>
    <cbc:ID>{{ doc.series }}-{{ doc.number }}</cbc:ID>
    <cbc:IssueDate>{{ doc.date_of_issue }}</cbc:IssueDate>
    <cbc:IssueTime>{{ doc.time_of_issue }}</cbc:IssueTime>
    {% if invoice.date_of_due -%}
    <cbc:DueDate>{{ invoice.date_of_due }}</cbc:DueDate>
    {% endif -%}
    <cbc:InvoiceTypeCode listID="{{ invoice.operation_type_id }}">{{ doc.document_type_id }}</cbc:InvoiceTypeCode>
    {% for leg in doc.legends -%}
    <cbc:Note languageLocaleID="{{ leg.code }}"><![CDATA[{{ leg.value }}]]></cbc:Note>
    {% endfor -%}
    <cbc:DocumentCurrencyCode>{{ doc.currency_type_id }}</cbc:DocumentCurrencyCode>
    {% if doc.purchase_order -%}
    <cac:OrderReference>
        <cbc:ID>{{ doc.purchase_order }}</cbc:ID>
    </cac:OrderReference>
    {% endif -%}
    {% if doc.guides -%}
    {% for guide in doc.guides -%}
    <cac:DespatchDocumentReference>
        <cbc:ID>{{ guide.number }}</cbc:ID>
        <cbc:DocumentTypeCode>{{ guide.document_type_id }}</cbc:DocumentTypeCode>
    </cac:DespatchDocumentReference>
    {% endfor -%}
    {% endif -%}
    {% if doc.related_documents -%}
    {% for rel in doc.related_documents -%}
    <cac:AdditionalDocumentReference>
        <cbc:ID>{{ rel.number }}</cbc:ID>
        <cbc:DocumentTypeCode>{{ rel.document_type_id }}</cbc:DocumentTypeCode>
    </cac:AdditionalDocumentReference>
    {% endfor -%}
    {% endif -%}
    {% if invoice.prepayments -%}
    {% for prepayment in doc.prepayments -%}
    <cac:AdditionalDocumentReference>
        <cbc:ID>{{ prepayment.number }}</cbc:ID>
        <cbc:DocumentTypeCode>{{ prepayment.document_type_id }}</cbc:DocumentTypeCode>
        <cbc:DocumentStatusCode>{{ loop.index }}</cbc:DocumentStatusCode>
        <cac:IssuerParty>
            <cac:PartyIdentification>
                <cbc:ID schemeID="6">{{ company.number }}</cbc:ID>
            </cac:PartyIdentification>
        </cac:IssuerParty>
    </cac:AdditionalDocumentReference>
    {% endfor -%}
    {% endif -%}
    <cac:Signature>
        <cbc:ID>{{ company.ruc }}</cbc:ID>
        <cbc:Note>FACTURALO</cbc:Note>
        <cac:SignatoryParty>
            <cac:PartyIdentification>
                <cbc:ID>{{ company.ruc }}</cbc:ID>
            </cac:PartyIdentification>
            <cac:PartyName>
                <cbc:Name><![CDATA[{{ company.trade_name|raw }}]]></cbc:Name>
            </cac:PartyName>
        </cac:SignatoryParty>
        <cac:DigitalSignatureAttachment>
            <cac:ExternalReference>
                <cbc:URI>#FACTURALO-PERU</cbc:URI>
            </cac:ExternalReference>
        </cac:DigitalSignatureAttachment>
    </cac:Signature>
    <cac:AccountingSupplierParty>
        <cac:Party>
            <cac:PartyIdentification>
                <cbc:ID schemeID="6">{{ company.ruc }}</cbc:ID>
            </cac:PartyIdentification>
            <cac:PartyName>
                <cbc:Name><![CDATA[{{ company.trade_name|raw }}]]></cbc:Name>
            </cac:PartyName>
            <cac:PartyLegalEntity>
                <cbc:RegistrationName><![CDATA[{{ company.name|raw }}]]></cbc:RegistrationName>
                {% set establishment = doc.establishment -%}
                <cac:RegistrationAddress>
                    <cbc:ID>{{ establishment.district_id }}</cbc:ID>
                    <cbc:AddressTypeCode>{{ establishment.code }}</cbc:AddressTypeCode>
                    {% if establishment.urbanization -%}
                    <cbc:CitySubdivisionName>{{ establishment.urbanization }}</cbc:CitySubdivisionName>
                    {% endif -%}
                    <cbc:CityName>{{ establishment.province.description }}</cbc:CityName>
                    <cbc:CountrySubentity>{{ establishment.department.description }}</cbc:CountrySubentity>
                    <cbc:District>{{ establishment.district.description }}</cbc:District>
                    <cac:AddressLine>
                        <cbc:Line><![CDATA[{{ establishment.address|raw }}]]></cbc:Line>
                    </cac:AddressLine>
                    <cac:Country>
                        <cbc:IdentificationCode>{{ establishment.country_id }}</cbc:IdentificationCode>
                    </cac:Country>
                </cac:RegistrationAddress>
            </cac:PartyLegalEntity>
            {% if establishment.email establishment company.telephone -%}
            <cac:Contact>
                {% if establishment.telephone -%}
                <cbc:Telephone>{{ establishment.telephone }}</cbc:Telephone>
                {% endif -%}
                {% if establishment.email -%}
                <cbc:ElectronicMail>{{ establishment.email }}</cbc:ElectronicMail>
                {% endif -%}
            </cac:Contact>
            {% endif -%}
        </cac:Party>
    </cac:AccountingSupplierParty>
    {% set customer = doc.customer -%}
    <cac:AccountingCustomerParty>
        <cac:Party>
            <cac:PartyIdentification>
                <cbc:ID schemeID="{{ customer.identity_document_type_id }}">{{ customer.number }}</cbc:ID>
            </cac:PartyIdentification>
            <cac:PartyLegalEntity>
                <cbc:RegistrationName><![CDATA[{{ customer.name|raw }}]]></cbc:RegistrationName>
                {% if customer.address -%}
                <cac:RegistrationAddress>
                    {% if customer.district_id -%}
                    <cbc:ID>{{ customer.district_id }}</cbc:ID>
                    {% endif -%}
                    <cac:AddressLine>
                        <cbc:Line><![CDATA[{{ customer.address|raw }}]]></cbc:Line>
                    </cac:AddressLine>
                    <cac:Country>
                        <cbc:IdentificationCode>{{ customer.country_id }}</cbc:IdentificationCode>
                    </cac:Country>
                </cac:RegistrationAddress>
                {% endif -%}
            </cac:PartyLegalEntity>
            {% if customer.email or customer.telephone -%}
            <cac:Contact>
                {% if customer.telephone -%}
                <cbc:Telephone>{{ customer.telephone }}</cbc:Telephone>
                {% endif -%}
                {% if customer.email -%}
                <cbc:ElectronicMail>{{ customer.email }}</cbc:ElectronicMail>
                {% endif -%}
            </cac:Contact>
            {% endif -%}
        </cac:Party>
    </cac:AccountingCustomerParty>
    {% if invoice.detraction -%}
    {% set detraction = doc.detraction -%}
    <cac:PaymentMeans>
        <cbc:PaymentMeansCode>{{ detraction.payment_method_id }}</cbc:PaymentMeansCode>
        <cac:PayeeFinancialAccount>
            <cbc:ID>{{ detraction.bank_account }}</cbc:ID>
        </cac:PayeeFinancialAccount>
    </cac:PaymentMeans>
    <cac:PaymentTerms>
        <cbc:PaymentMeansID>{{ detraction.detraction_type_id }}</cbc:PaymentMeansID>
        <cbc:PaymentPercent>{{ detraction.percentage }}</cbc:PaymentPercent>
        <cbc:Amount currencyID="PEN">{{ detraction.amount|n_format }}</cbc:Amount>
    </cac:PaymentTerms>
    {% endif -%}
    {% if invoice.perception -%}
    {% set perception = doc.perception -%}
    <cac:PaymentTerms>
        <cbc:ID>Percepcion</cbc:ID>
        <cbc:Amount currencyID="PEN">{{ perception.amount|n_format }}</cbc:Amount>
    </cac:PaymentTerms>
    {% endif -%}
    {% if invoice.prepayments -%}
    {% for prepayment in doc.prepayments -%}
    <cac:PrepaidPayment>
        <cbc:ID>{{ loop.index }}</cbc:ID>
        <cbc:PaidAmount currencyID="{{ doc.currency_type_id }}">{{ prepayment.amount|n_format }}</cbc:PaidAmount>
    </cac:PrepaidPayment>
    {% endfor -%}
    {% endif -%}
    {% if invoice.charges -%}
    {% for charge in doc.charges -%}
    <cac:AllowanceCharge>
        <cbc:ChargeIndicator>true</cbc:ChargeIndicator>
        <cbc:AllowanceChargeReasonCode>{{ charge.code }}</cbc:AllowanceChargeReasonCode>
        <cbc:MultiplierFactorNumeric>{{ charge.percentage }}</cbc:MultiplierFactorNumeric>
        <cbc:Amount currencyID="{{ doc.currency_type_id }}">{{ charge.amount|n_format }}</cbc:Amount>
        <cbc:BaseAmount currencyID="{{ doc.currency_type_id }}">{{ charge.base|n_format }}</cbc:BaseAmount>
    </cac:AllowanceCharge>
    {% endfor -%}
    {% endif -%}
    {% if invoice.discounts -%}
    {% for discount in doc.discounts -%}
    <cac:AllowanceCharge>
        <cbc:ChargeIndicator>false</cbc:ChargeIndicator>
        <cbc:AllowanceChargeReasonCode>{{ discount.code }}</cbc:AllowanceChargeReasonCode>
        <cbc:MultiplierFactorNumeric>{{ discount.percentage }}</cbc:MultiplierFactorNumeric>
        <cbc:Amount currencyID="{{ doc.currency_type_id }}">{{ discount.amount|n_format }}</cbc:Amount>
        <cbc:BaseAmount currencyID="{{ doc.currency_type_id }}">{{ discount.base|n_format }}</cbc:BaseAmount>
    </cac:AllowanceCharge>
    {% endfor -%}
    {% endif -%}
    {% if invoice.perception -%}
    {% set perception = invoice.perception -%}
    <cac:AllowanceCharge>
        <cbc:ChargeIndicator>true</cbc:ChargeIndicator>
        <cbc:AllowanceChargeReasonCode>{{ perception.code }}</cbc:AllowanceChargeReasonCode>
        <cbc:MultiplierFactorNumeric>{{ perception.percentage }}</cbc:MultiplierFactorNumeric>
        <cbc:Amount currencyID="PEN">{{ perception.amount|n_format }}</cbc:Amount>
        <cbc:BaseAmount currencyID="PEN">{{ perception.base|n_format }}</cbc:BaseAmount>
    </cac:AllowanceCharge>
    {% endif -%}
    <cac:TaxTotal>
        <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_taxes|n_format }}</cbc:TaxAmount>
        {% if doc.total_isc -%}
        <cac:TaxSubtotal>
            <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_base_isc|n_format }}</cbc:TaxableAmount>
            <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_isc|n_format }}</cbc:TaxAmount>
            <cac:TaxCategory>
                <cac:TaxScheme>
                    <cbc:ID>2000</cbc:ID>
                    <cbc:Name>ISC</cbc:Name>
                    <cbc:TaxTypeCode>EXC</cbc:TaxTypeCode>
                </cac:TaxScheme>
            </cac:TaxCategory>
        </cac:TaxSubtotal>
        {% endif -%}
        {% if doc.total_taxed -%}
        <cac:TaxSubtotal>
            <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_taxed|n_format }}</cbc:TaxableAmount>
            <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_igv|n_format }}</cbc:TaxAmount>
            <cac:TaxCategory>
                <cac:TaxScheme>
                    <cbc:ID>1000</cbc:ID>
                    <cbc:Name>IGV</cbc:Name>
                    <cbc:TaxTypeCode>VAT</cbc:TaxTypeCode>
                </cac:TaxScheme>
            </cac:TaxCategory>
        </cac:TaxSubtotal>
        {% endif -%}
        {% if doc.total_unaffected -%}
            <cac:TaxSubtotal>
                <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_unaffected|n_format }}</cbc:TaxableAmount>
                <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">0</cbc:TaxAmount>
                <cac:TaxCategory>
                    <cac:TaxScheme>
                        <cbc:ID>9998</cbc:ID>
                        <cbc:Name>INA</cbc:Name>
                        <cbc:TaxTypeCode>FRE</cbc:TaxTypeCode>
                    </cac:TaxScheme>
                </cac:TaxCategory>
            </cac:TaxSubtotal>
        {% endif -%}
        {% if doc.total_exonerated -%}
            <cac:TaxSubtotal>
                <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_exonerated|n_format }}</cbc:TaxableAmount>
                <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">0</cbc:TaxAmount>
                <cac:TaxCategory>
                    <cac:TaxScheme>
                        <cbc:ID>9997</cbc:ID>
                        <cbc:Name>EXO</cbc:Name>
                        <cbc:TaxTypeCode>VAT</cbc:TaxTypeCode>
                    </cac:TaxScheme>
                </cac:TaxCategory>
            </cac:TaxSubtotal>
        {% endif -%}
        {% if doc.total_free -%}
            <cac:TaxSubtotal>
                <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_free|n_format }}</cbc:TaxableAmount>
                <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">0</cbc:TaxAmount>
                <cac:TaxCategory>
                    <cac:TaxScheme>
                        <cbc:ID>9996</cbc:ID>
                        <cbc:Name>GRA</cbc:Name>
                        <cbc:TaxTypeCode>FRE</cbc:TaxTypeCode>
                    </cac:TaxScheme>
                </cac:TaxCategory>
            </cac:TaxSubtotal>
        {% endif -%}
        {% if doc.total_exportation -%}
            <cac:TaxSubtotal>
                <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_exportation|n_format }}</cbc:TaxableAmount>
                <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">0</cbc:TaxAmount>
                <cac:TaxCategory>
                    <cac:TaxScheme>
                        <cbc:ID>9995</cbc:ID>
                        <cbc:Name>EXP</cbc:Name>
                        <cbc:TaxTypeCode>FRE</cbc:TaxTypeCode>
                    </cac:TaxScheme>
                </cac:TaxCategory>
            </cac:TaxSubtotal>
        {% endif -%}
        {% if doc.total_other_taxes -%}
        <cac:TaxSubtotal>
            <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_other_taxes|n_format }}</cbc:TaxableAmount>
            <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_base_other_taxes|n_format }}</cbc:TaxAmount>
            <cac:TaxCategory>
                <cac:TaxScheme>
                    <cbc:ID>9999</cbc:ID>
                    <cbc:Name>OTROS</cbc:Name>
                    <cbc:TaxTypeCode>OTH</cbc:TaxTypeCode>
                </cac:TaxScheme>
            </cac:TaxCategory>
        </cac:TaxSubtotal>
        {% endif -%}
    </cac:TaxTotal>
    <cac:LegalMonetaryTotal>
        <cbc:LineExtensionAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_value|n_format }}</cbc:LineExtensionAmount>
        {% if doc.total_discount -%}
        <cbc:AllowanceTotalAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_discount|n_format }}</cbc:AllowanceTotalAmount>
        {% endif -%}
        {% if doc.total_other_charges -%}
        <cbc:ChargeTotalAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_other_charges|n_format }}</cbc:ChargeTotalAmount>
        {% endif -%}
        {% if doc.total_prepayment -%}
        <cbc:PrepaidAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total_prepayment|n_format }}</cbc:PrepaidAmount>
        {% endif -%}
        <cbc:PayableAmount currencyID="{{ doc.currency_type_id }}">{{ doc.total|n_format }}</cbc:PayableAmount>
    </cac:LegalMonetaryTotal>
    {% for detail in doc.details -%}
    <cac:InvoiceLine>
        <cbc:ID>{{ loop.index }}</cbc:ID>
        <cbc:InvoicedQuantity unitCode="{{ detail.unit_type_id }}">{{ detail.quantity }}</cbc:InvoicedQuantity>
        <cbc:LineExtensionAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_value|n_format }}</cbc:LineExtensionAmount>
        <cac:PricingReference>
            <cac:AlternativeConditionPrice>
                <cbc:PriceAmount currencyID="{{ doc.currency_type_id }}">{{ detail.unit_price|n_format(6) }}</cbc:PriceAmount>
                <cbc:PriceTypeCode>{{ detail.price_type_id }}</cbc:PriceTypeCode>
            </cac:AlternativeConditionPrice>
        </cac:PricingReference>
        {% if detail.charges -%}
        {% for charge in detail.charges -%}
        <cac:AllowanceCharge>
            <cbc:ChargeIndicator>true</cbc:ChargeIndicator>
            <cbc:AllowanceChargeReasonCode>{{ charge.code }}</cbc:AllowanceChargeReasonCode>
            <cbc:MultiplierFactorNumeric>{{ charge.percentage }}</cbc:MultiplierFactorNumeric>
            <cbc:Amount currencyID="{{ doc.currency_type_id }}">{{ charge.amount }}</cbc:Amount>
            <cbc:BaseAmount currencyID="{{ doc.currency_type_id }}">{{ charge.base }}</cbc:BaseAmount>
        </cac:AllowanceCharge>
        {% endfor -%}
        {% endif -%}
        {% if detail.descuentos -%}
        {% for discount in detail.descuentos -%}
        <cac:AllowanceCharge>
            <cbc:ChargeIndicator>false</cbc:ChargeIndicator>
            <cbc:AllowanceChargeReasonCode>{{ discount.code }}</cbc:AllowanceChargeReasonCode>
            <cbc:MultiplierFactorNumeric>{{ discount.percentage }}</cbc:MultiplierFactorNumeric>
            <cbc:Amount currencyID="{{ doc.currency_type_id }}">{{ discount.amount }}</cbc:Amount>
            <cbc:BaseAmount currencyID="{{ doc.currency_type_id }}">{{ discount.base }}</cbc:BaseAmount>
        </cac:AllowanceCharge>
        {% endfor -%}
        {% endif -%}
        <cac:TaxTotal>
            <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_taxes|n_format }}</cbc:TaxAmount>
            {% if detail.total_isc -%}
            <cac:TaxSubtotal>
                <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_base_isc|n_format }}</cbc:TaxableAmount>
                <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_isc|n_format }}</cbc:TaxAmount>
                <cac:TaxCategory>
                    <cbc:Percent>{{ detail.percentage_isc }}</cbc:Percent>
                    <cbc:TierRange>{{ detail.system_isc_type_id }}</cbc:TierRange>
                    <cac:TaxScheme>
                        <cbc:ID>2000</cbc:ID>
                        <cbc:Name>ISC</cbc:Name>
                        <cbc:TaxTypeCode>EXC</cbc:TaxTypeCode>
                    </cac:TaxScheme>
                </cac:TaxCategory>
            </cac:TaxSubtotal>
            {% endif -%}
            <cac:TaxSubtotal>
                <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_base_igv|n_format }}</cbc:TaxableAmount>
                <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_igv|n_format }}</cbc:TaxAmount>
                <cac:TaxCategory>
                    <cbc:Percent>{{ detail.percentage_igv }}</cbc:Percent>
                    <cbc:TaxExcompanytionReasonCode>{{ detail.affectation_igv_type_id }}</cbc:TaxExcompanytionReasonCode>
                    {% set affectation = getTributeFunction(detail.affectation_igv_type_id) -%}
                    <cac:TaxScheme>
                        <cbc:ID>{{ affectation.id }}</cbc:ID>
                        <cbc:Name>{{ affectation.name }}</cbc:Name>
                        <cbc:TaxTypeCode>{{ affectation.code }}</cbc:TaxTypeCode>
                    </cac:TaxScheme>
                </cac:TaxCategory>
            </cac:TaxSubtotal>
            {% if detail.total_other_taxes -%}
                <cac:TaxSubtotal>
                    <cbc:TaxableAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_base_other_taxes|n_format }}</cbc:TaxableAmount>
                    <cbc:TaxAmount currencyID="{{ doc.currency_type_id }}">{{ detail.total_other_taxes|n_format }}</cbc:TaxAmount>
                    <cac:TaxCategory>
                        <cbc:Percent>{{ detail.percentage_other_taxes }}</cbc:Percent>
                        <cac:TaxScheme>
                            <cbc:ID>9999</cbc:ID>
                            <cbc:Name>OTROS</cbc:Name>
                            <cbc:TaxTypeCode>OTH</cbc:TaxTypeCode>
                        </cac:TaxScheme>
                    </cac:TaxCategory>
                </cac:TaxSubtotal>
            {% endif -%}
        </cac:TaxTotal>
        <cac:Item>
            <cbc:Description><![CDATA[{{ detail.item_description|raw }}]]></cbc:Description>
            {% if detail.internal_id -%}
            <cac:SellersItemIdentification>
                <cbc:ID>{{ detail.internal_id }}</cbc:ID>
            </cac:SellersItemIdentification>
            {% endif -%}
            {% if detail.item_code -%}
            <cac:CommodityClassification>
                <cbc:ItemClassificationCode>{{ detail.item_code }}</cbc:ItemClassificationCode>
            </cac:CommodityClassification>
            {% endif -%}
            {% if detail.item_code_gs1 -%}
            <cac:StandardItemIdentification>
                <cbc:ID>{{ detail.item_code_gs1 }}</cbc:ID>
            </cac:StandardItemIdentification>
            {% endif -%}
            {% if detail.attributes -%}
                {% for attr in detail.attributes -%}
                    <cac:AdditionalItcompanyroperty >
                        <cbc:Name>{{ attr.name }}</cbc:Name>
                        <cbc:NameCode>{{ attr.code }}</cbc:NameCode>
                        {% if attr.value -%}
                        <cbc:Value>{{ attr.value }}</cbc:Value>
                        {% endif -%}
                        {% if attr.start_date or attr.end_date or attr.duration -%}
                            <cac:UsabilityPeriod>
                                {% if attr.start_date -%}
                                <cbc:StartDate>{{ attr.start_date|date('Y-m-d') }}</cbc:StartDate>
                                {% endif -%}
                                {% if attr.end_date -%}
                                <cbc:EndDate>{{ attr.end_date|date('Y-m-d') }}</cbc:EndDate>
                                {% endif -%}
                                {% if attr.duration -%}
                                <cbc:DurationMeasure unitCode="DAY">{{ attr.duration }}</cbc:DurationMeasure>
                                {% endif -%}
                            </cac:UsabilityPeriod>
                        {% endif -%}
                    </cac:AdditionalItcompanyroperty>
                {% endfor -%}
            {% endif -%}
        </cac:Item>
        <cac:Price>
            <cbc:PriceAmount currencyID="{{ doc.currency_type_id }}">{{ detail.unit_value|n_format(6) }}</cbc:PriceAmount>
        </cac:Price>
    </cac:InvoiceLine>
    {% endfor -%}
</Invoice>
